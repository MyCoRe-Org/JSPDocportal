<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="org.mycore.backend.hibernate.tables.MCRRESUMPTIONTOKEN_HIT" table="MCRRESUMPTIONTOKEN_HIT">
      <id column="HITID" name="hitID" type="long">
         <generator class="native" />
      </id>
      <property name="hitNr" type="long" column="HITNR" not-null="true" unique="false" />      
      <property name="mcrobjID" type="string" column="MCROBJID" length="64" unique="false" />
      <property name="oaiID" type="string" column="OAIID" length="64" unique="false" />      
      <property name="spec" type="string" column="SPEC" length="255" unique="false" />
      <property name="specName" type="string" column="SPECNAME" length="400" unique="false" />            
      <property name="specDescription" type="string" column="SPECDESC" length="400" unique="false" />      
      <property name="datestamp" type="string" column="DATESTAMP" unique="false" />
      <many-to-one name="resumptionToken" 
         	class="org.mycore.backend.hibernate.tables.MCRRESUMPTIONTOKEN" 
            column="RESUMPTIONTOKENID" not-null="true" />
   </class>  
</hibernate-mapping>
