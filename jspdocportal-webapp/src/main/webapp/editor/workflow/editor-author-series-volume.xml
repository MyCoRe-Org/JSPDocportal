<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE editor>

<editor>
   	 <source uri="{sourceUri}" />
     <cancel url="{cancelUrl}" />

<!--
 <target type="debug" />
-->

 <target type="servlet" name="MCRPassToTargetServlet" method="post" format="xml" />
  <validationMessage>
   <label i18n="Editor.Common.validationMessageLong" />
 </validationMessage>

 <components root="author-series" var="/mycoreobject">

  <include uri="webapp:editor/workflow/imports-document.xml" />    

  <headline anchor="CENTER">
   <text i18n="Editor.Common.mandatoryFields" />
  </headline>

  <panel id="author-series">
    <hidden var="ID" />
    <hidden var="label" />
    
    <cell col="1" row="1" anchor="NORTHWEST" ref="LPublicationType" />
    <cell col="2" row="1" anchor="NORTHEAST" ref="HPublicationType" />
    <cell col="1" row="2" colspan="2" anchor="NORTHWEST" ref="DType_SV" sortnr="10" />
    
    <cell col="1" row="10"  colspan="2" anchor="WEST" ><hr/> </cell>

    <cell col="1" row="11" anchor="NORTHWEST" ref="LTitleP" />
    <cell col="2" row="11" anchor="NORTHEAST" ref="HTitle" />

    <cell col="1" row="12" colspan="2" anchor="NORTHWEST" ref="DTitle_SV" sortnr="11" />


    <cell col="1" row="41" anchor="NORTHWEST" ref="LPublishingTime_S" />
    <cell col="2" row="41" anchor="NORTHEAST" ref="PublishingYear" />
    <cell col="1" row="42" colspan="2" anchor="NORTHWEST" ref="DDate_From_To" sortnr="21"/>

    <cell col="1" row="61" anchor="NORTHWEST" ref="LIdentifier" />
    <cell col="2" row="61" anchor="NORTHEAST" ref="HIdentifier" /> 
	<cell col="1" row="62" colspan="2" anchor="NORTHWEST" ref="DIdentifier_S" sortnr="32" />
	
	<cell col="1" row="80"  colspan="2" anchor="WEST" ><hr/> </cell>

	<cell col="1" row="81" anchor="NORTHWEST" ref="LParent"  />
    <cell col="2" row="81" anchor="WEST" ref="HParent" />
    <cell col="1" row="82" anchor="NORTHWEST" ref="DParent" sortnr="2"  />

    <cell col="1" row="86" anchor="NORTHWEST" ref="LParts_S"  />
    <cell col="2" row="86" anchor="WEST" ref="HParts_S" />
    <cell col="1" row="87" anchor="NORTHWEST" ref="DParts_S" sortnr="1"  />

    <cell col="1" row="90" colspan="2" anchor="NORTHWEST" ref="DHiddenFields" />
    <hidden var="metadata/mappings" descendants="true" />
    <cell col="1" row="91" anchor="EAST" ><space width="10" height="40" /></cell>
    <cell col="1" row="95" anchor="EAST" ref="BCancel"            />
    <cell col="2" row="95" anchor="WEST" ref="BSubmit" var="save" />
  </panel>
  
  <text id="LPublishingTime_S" i18n="Editor.Label.Time" />
  <text id="LParts_S" i18n="Editor.Label.Parts" />
  
  	<helpHover id="HParts_S" i18n="Editor.Help.HParts" />

 
    <panel id="DParts_S">
    <include uri="localclass:org.mycore.frontend.editor.helper.MCRGetEditorElements?mode=getHiddenAttributesForClass&amp;var=structure/children&amp;classname=MCRMetaLinkID" /> 
    <cell col="1" row="1" anchor="NORTHWEST" var="structure/children/child"  >
        <repeater min="1" max="30">
          <panel lines="off">
            <hidden var="@inherited" default="0"  />
            <hidden var="@xlink:type" default="locator"  />
            <hidden var="@xlink:title" default="Einheit" />
            <hidden var="@xlink:href" default="" />
            <cell row="1" col="1" width="100%">
              <output var="@xlink:title" default=" Einheit " />
              <text label="&#160;&#160;&#160;" />
            </cell>
          </panel>
        </repeater>
      </cell>
  </panel>
  
  
   <panel id="DTitle_SV">
    <hidden var="metadata/titles/@class" default="MCRMetaLangText" />
    <hidden var="metadata/titles/@notinherit" default="false" />
    <hidden var="metadata/titles/@heritable" default="false" />
    <cell col="1" row="1" var="metadata/titles/title" >
       <repeater min="1" max="10" ref="RTitle" />
    </cell>
  </panel>
	<panel id="DDate_From_To">
        <hidden var="metadata/dates/@class" default="MCRMetaHistoryDate" />
        <cell col="1" row="1" var="metadata/dates/date" >
			  <panel>
			      	<cell col="1" row="1" anchor="NORTHWEST" var="text">
      					<textfield width="25" maxlength="64" />
		    	 	</cell>
			      	<cell col="2" row="1" anchor="NORTHWEST">
      					<text class="editorHint" i18n="Editor.Label.DateShown" />
	      			</cell>
	      			<cell col="3" row="1" anchor="NORTHWEST" var="von">
    	  				<textfield  width="13" maxlength="13" />
					</cell>
					<cell col="4" row="1" anchor="NORTHWEST">
			      		<text class="editorHint" i18n="Editor.Label.DateFrom" />
      				</cell>

		      		<cell col="5" row="1" anchor="NORTHWEST" var="bis">
	      				<textfield width="13" maxlength="13" />
				   	</cell>
		    	  	<cell col="6" row="1" anchor="NORTHWEST">
	      				<text class="editorHint" i18n="Editor.Label.DateTo" />
		    	  	</cell>
	      	</panel>

        </cell>
      </panel>
	
  <panel id="DType_SV">
    <include uri="localclass:org.mycore.frontend.editor.helper.MCRGetEditorElements?mode=getHiddenAttributesForClass&amp;var=metadata/types&amp;classname=MCRMetaClassification" />
    <include uri="localclass:org.mycore.frontend.editor.helper.MCRGetEditorElements?mode=getHiddenVar&amp;var=metadata/types/type/@classid&amp;prop=MCR.ClassificationID.Type&amp;defaultValue=DocPortal_class_00000005" />               
    <cell col="1" row="1" anchor="WEST" var="metadata/types/type/@categid">
  	<list width="425px" type="dropdown" class="mandatory">
      	<include uri="localclass:org.mycore.frontend.editor.helper.MCRGetEditorElements?mode=getSpecialCategoriesInItems&amp;classProp=MCR.ClassificationID.Type&amp;categoryProp=MCR.Classification.Type.SelectCategoryIDs.series-volume&amp;withCounter=false" />                       
     	<condition id="CTypeSelected" regexp=".+" required="true" >
				<label i18n="Editor.Validate.Selection.Empty" />
     	</condition>
	  </list>  
    </cell>
  </panel>


<!-- *** Data for the Identifier ** sortnr="15"-->
    <panel id="DIdentifier_S_repeater">
      <hidden var="@inherited" default="0"  />
      <hidden var="@xml:lang" default="de"  />
      <cell col="1" row="1" anchor="WEST" ref="TFzeile128"  />
      <cell col="2" row="1" anchor="NORTHWEST" var="@type">
      	<list type="dropdown">
      		<item value="free" i18n="OMD.id.free" />
      		<item value="gbv" i18n="OMD.id.gbv" />
      		<item value="gbv_original" i18n="OMD.id.gbv_original" />
      	</list>
      </cell>      
    </panel>

    <panel id="DIdentifier_S">
      <include uri="localclass:org.mycore.frontend.editor.helper.MCRGetEditorElements?mode=getHiddenAttributesForClass&amp;var=metadata/identifiers&amp;classname=MCRMetaLangText" />
      <cell col="1" row="1" anchor="NORTHWEST" var="metadata/identifiers/identifier"  >
        <repeater min="1" max="5" ref="DIdentifier_S_repeater"  />
      </cell>      
    </panel>
 </components>
</editor>

