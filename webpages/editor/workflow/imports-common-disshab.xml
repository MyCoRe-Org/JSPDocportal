<!-- *** Data for the TITLE *** -->

  <text id="LTitleDissP">
    <label xml:lang="de">
      <span class="editorlabel">Titel der Arbeit (*)</span>
    </label>
    <label xml:lang="en">
      <span class="editorlabel">Paper Title (*)</span>
    </label>
  </text>

<!-- *** Data for the TITLE ** -->

  <panel id="RTitle">
    <hidden var="@inherited" default="0" />
    <cell col="1" row="1" anchor="NORTHWEST" ref="TFzeile1024M" />
    <cell col="2" row="1" anchor="NORTHWEST" ref="Slang" var="@lang" default="de" />
    <cell col="3" row="1" anchor="NORTHWEST" ref="STitleType" var="@type" default="main" />
  </panel>

  <panel id="DTitle">
    <hidden var="metadata/titles/@class" default="MCRMetaLangText" />
    <hidden var="metadata/titles/@parasearch" default="true" />
    <hidden var="metadata/titles/@textsearch" default="true" />
    <hidden var="metadata/titles/@notinherit" default="false" />
    <hidden var="metadata/titles/@heritable" default="true" />

    <cell col="1" row="1" var="metadata/titles/title" >
       <repeater min="4" max="15" ref="RTitle" />
    </cell>

  </panel>

