<!-- für Dissertationen STANDARD AUSSCHLIESSLICH CREATORLINK -->
   <text id="LCreatorLinks">
        <label xml:lang="de">
          <span class="editorlabel">Autor</span>
        </label>
   </text>
   <panel id="DCreatorLinks">
        <hidden var="metadata/creatorlinks/@class" default="MCRMetaLinkID" sortnr="3" />
	    <cell col="1" row="1" anchor="EAST" var="metadata/creatorlinks/creatorlink" sortnr="3" >
         <repeater min="1" max="10">
           <panel lines="off">
			 <hidden var="@inherited" default="0" />
			 <hidden var="@type" default="locator" />
			 <hidden var="@title" />
			 <hidden var="@href" />			
             <cell row="1" col="1" width="100%">
               <output var="@title" default="Autor"/>
               <text   label=" (" /><output var="@href" /><text   label=")" />
             </cell>
			 <cell row="1" col="2" >
               <subselect label="Auswählen" id="sub.select" type="webpage" href="nav?path=~indexSearchCreatorsSub" />
             </cell>			   
           </panel>
         </repeater>
       </cell>	  
   </panel>
	 


<!-- ** Data for the SUBJECT *** -->

     <text id="LSubjects">
        <label xml:lang="de">
          <span class="editorlabel">
            Klassifikation
          </span>
        </label>
     </text>
     <panel id="DSubjects" >
        <hidden var="metadata/subjects/@class" default="MCRMetaClassification" />
        <cell row="1" col="1" anchor="NORTHWEST" var="metadata/subjects/subject">
           <repeater min="1" max="5" >  
                <panel lines="off">
                 <hidden var="@inherited" default="0" />
                 <hidden var="@type" default="locator" />
                 <!-- configure this in mycore.properties  -->
                 <include uri="request:editor/helper/getHiddenVarElement.jspx?var=@classid&amp;prop=MCR.WorkflowEngine.ClassificationID.DDC.xmetadiss&amp;defaultValue=DocPortal_class_00000009" />                
                <!--  <hidden var="@classid" default="DocPortal_class_00000009" />  -->
                 <hidden var="@categid" />
                 <cell row="1" col="1" width="100%">
                   <output var="@title" default="DDC Klassifikation"/> <text label=" (" /><output var="@categid" default=" " /><text label=")" />
                   <text   label=" " />
                   <output var="@href" />
                 </cell>
                 <cell row="1" col="2">
                   <subselect label="Auswählen" id="sub.select2" type="webpage" href="nav?path=~browse_subDDC" />
                 </cell>
               </panel>
            </repeater>
        </cell>
      </panel>



