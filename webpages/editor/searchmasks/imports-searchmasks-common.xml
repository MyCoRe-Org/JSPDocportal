            <label xml:lang="de">   Es ist die Fakultät einzugeben.
            </label>
            <label xml:lang="en">   Input the faculty. 
            </label>
      </helpPopup>
      <list id="COrigins" width="300" type="dropdown">
            <item value="">
                  <label xml:lang="de">(bitte wählen)</label>
	              <label xml:lang="en">(please choose)</label>
            </item>
            <include   uri="request:content/classifications-to-items.jspx?classid=DocPortal_class_00000003" />
      </list>      
      
      <!-- *** Data for the Types ** -->
      <text id="LTypes">
            <label xml:lang="de"> <span class="editorlabel">&#160;&#160;&#160;Typ</span>  </label>
