            <hidden var="@operator" default="like" />            
            <cell col="1" row="1" anchor="NORTHWEST" ref="TFzeile128" var="@value" />
      </panel>
      <helpPopup id="HMetadata" width="400px" height="250px">
            <label xml:lang="de">    Hier können Sie einfach Begriffe eingeben, 
                                     die im gesamten Metadatensatz gesucht werden. 
                                     Achtung, dieser Begriff wird nicht im 
                                     eigentlichen Dokument gesucht!            </label>
            <label xml:lang="en">    Enter a word that the system should find 
                                     in the metadata set.            </label>
      </helpPopup>
      <!-- *** Data for the Content ** -->
      <text id="LContents">
            <label xml:lang="de">  <span class="editorlabel">&#160;&#160;&#160;Volltext in den Dokumenten</span>  </label>
