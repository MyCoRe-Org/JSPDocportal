            <label xml:lang="de">   Es ist der Titel des Objektes einzugeben.
            </label>
            <label xml:lang="en">   Input the title of this object. 
            </label>
      </helpPopup>
      <!-- *** Data for the Creator ** -->
      <text id="LCreators">
            <label xml:lang="de">
                  <span class="editorlabel">&#160;&#160;&#160;Autor</span>
            </label>
            <label xml:lang="en">
