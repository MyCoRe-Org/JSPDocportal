      <!-- *** Data for the Metadata ** -->
      <text id="LMetadata">
            <label xml:lang="de"><span class="editorlabel">&#160;&#160;&#160;Volltext in den Metadaten</span> </label>
            <label xml:lang="en"><span class="editorlabel">&#160;&#160;&#160;Freetext in the metadata</span>  </label>
      </text>
      <text id="LMetadataFastSearch">
            <label xml:lang="de"><span class="editorlabel">&#160;&#160;&#160;Schnellsuche&#160;&#160;&#160;</span>  </label>
            <label xml:lang="en"><span class="editorlabel">&#160;&#160;&#160;Fast Search&#160;&#160;&#160;</span>   </label>
      </text>      
      <panel id="DMetadata">
