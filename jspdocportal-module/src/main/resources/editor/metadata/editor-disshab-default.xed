                      </select>
                    </xed:bind>
                </div>
                </div>
                <div class="row ir-row"> 
                  <div class="col-sm-10 col-sm-offset-2">
                    <xed:bind xpath=".">
                      <textarea id="abstract" class="form-control input-sm" rows="6" />
                    </xed:bind>
                  </div>
