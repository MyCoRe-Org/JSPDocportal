     <div class="form-group">
       <label class="col-sm-2 control-label input-sm ir-label" for="keyword">
         <xed:output i18n="OMD.ir.keywords" />
       </label>
       <div class="col-sm-10">
          <xed:repeat xpath="mods:abstract[@type='author_keywords']">
            <div class="row ir-row ir-row-1">
