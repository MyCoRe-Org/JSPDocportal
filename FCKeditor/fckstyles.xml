<Styles>
	<Style name="Table - Highlighted" element="table">
		<Attribute name="class" value="highlighted" />
	</Style>
	<Style name="Table - Normal" element="table">
		<Attribute name="class" value="" />
	</Style>
	<Style name="Text - Maintitle" element="span">
		<Attribute name="class" value="maintitle" />
	</Style>
	<Style name="Text - Subtitle" element="span">
		<Attribute name="class" value="subtitle" />
	</Style>
		<Style name="Text - Normal" element="span">
		<Attribute name="class" value="" />
	</Style>
</Styles>